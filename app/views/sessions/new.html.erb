<h1>Login</h1>

    <div>
      <h1>Log in</h1>
      <div>
        <%= form_for(:session, url: login_path) do |f| %>

          <%= f.label :email %>
          <%= f.text_field :email %>

          <%= f.label :password %>
          <%= f.password_field :password %>

          <%= f.submit "Log in" %>
        <% end %>

        <p><%= link_to "Home", root_path %></p>
      </div>
    </div>  